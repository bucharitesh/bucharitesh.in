{
  "name": "@bucharitesh/cli",
  "version": "0.0.2",
  "description": "A simple CLI wrapper for adding components from bucharitesh.in component registry using shadcn.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": "./dist/index.js",
  "files": ["dist"],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start:dev": "node dist/index.js",
    "publish:release": "pnpm build && pnpm publish --access public"
  },
  "keywords": ["bucharitesh", "cli", "shadcn", "components", "registry"],
  "author": "Bucharitesh <bucharitesh@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.21",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^13.0.0",
    "inquirer": "^12.9.6",
    "ora": "^9.0.0"
  }
}
